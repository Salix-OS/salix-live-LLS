#: ../../libexec/create-slxsave-xfs:30
#, sh-format
msgid "ERROR: You need to be root to run this"
msgstr "ΣΦΑΛΜΑ: Πρέπει να έχετε δικαιώματα χρήστη root"

#: ../../libexec/create-slxsave-xfs:35
#, sh-format
msgid "ERROR: zenity must be installed and in your PATH to run this"
msgstr "ΣΦΑΛΜΑ: Το zenity δεν να είναι εγκατεστημένο"

#: ../../libexec/create-slxsave-xfs:40
#, sh-format
msgid "ERROR: xfsprogs must be installed to run this"
msgstr "ΣΦΑΛΜΑ: Το xfsprogs δεν είναι εγκατεστημένο"

#: ../../libexec/create-slxsave-xfs:49
#, sh-format
msgid "Choose of mount point"
msgstr "Επιλογή σημείου προσάρτησης"

#: ../../libexec/create-slxsave-xfs:52
#, sh-format
msgid "Choose the mountpoint where to save the persistent file:"
msgstr "Επιλέξτε το σημείο προσάρτησης για την αποθήκευση του διαρκούς αρχείου"

#: ../../libexec/create-slxsave-xfs:53
#, sh-format
msgid "Mount point"
msgstr "Σημείο προσάρτησης"

#: ../../libexec/create-slxsave-xfs:54
#, sh-format
msgid "Size (in MB)"
msgstr "Μέγεθος (σε MB)"

#: ../../libexec/create-slxsave-xfs:69
#, sh-format
msgid "Choose of size"
msgstr "Επιλογή μεγέθους"

#: ../../libexec/create-slxsave-xfs:72
#, sh-format
msgid "Choose the size in megabytes (maximum $MAXSIZE) for persistent file on $MOUNTPOINT:"
msgstr "Επιλογή του μεγέθους σε MB (μέγιστο $MAXSIZE) για το διαρκές αρχείο στο $MOUNTPOINT:"

#: ../../libexec/create-slxsave-xfs:81
#: ../../libexec/create-slxsave-xfs:102
#, sh-format
msgid "Error"
msgstr "Σφάλμα"

#: ../../libexec/create-slxsave-xfs:84
#, sh-format
msgid "The chosen size is too big ($SIZE > $MAXSIZE)"
msgstr "Το μέγεθος είναι πολύ μεγάλο ($SIZE > $MAXSIZE)"

#: ../../libexec/create-slxsave-xfs:97
#, sh-format
msgid "slxsave.xfs ($SIZE MB) correctly created in $MOUNTPOINT"
msgstr "Το slxsave.xfs ($SIZE MB) δημιουργήθηκε με επιτυχία στο $MOUNTPOINT"

#: ../../libexec/create-slxsave-xfs:105
#, sh-format
msgid "slxsave.xfs ($SIZE MB) cannot be created in $MOUNTPOINT"
msgstr "Το slxsave.xfs ($SIZE MB) δεν μπορεί να δημιουργηθεί στο $MOUNTPOINT"

