#: create-slxsave-xfs:28
#, sh-format
msgid "ERROR: You need to be root to run this"
msgstr "ERREUR : Vous devez être root pour que ça s'exécute"

#: create-slxsave-xfs:33
#, sh-format
msgid "ERROR: zenity must be installed and in your PATH to run this"
msgstr "ERREUR : zenity doit être installé et dans votre PATH pour que ça s'exécute"

#: create-slxsave-xfs:38
#, sh-format
msgid "ERROR: xfsprogs must be installed to run this"
msgstr "ERREUR : xfsprogs doit êtres installé pour que ça s'exécute"

#: create-slxsave-xfs:47
#, sh-format
msgid "Mount point selection"
msgstr "Sélection du point de montage"

#: create-slxsave-xfs:50
#, sh-format
msgid "Choose the mountpoint where to save the persistent file:"
msgstr "Choisissez un point de montage où sauvegarder le fichier persistent :"

#: create-slxsave-xfs:51
#, sh-format
msgid "Mount point"
msgstr "Point de montage"

#: create-slxsave-xfs:52
#, sh-format
msgid "Size (in MB)"
msgstr "Taille (en Mo)"

#: create-slxsave-xfs:67
#, sh-format
msgid "Size selection"
msgstr "Sélection de la taille"

#: create-slxsave-xfs:70
#, sh-format
msgid "Choose the size in megabytes (maximum $MAXSIZE) for persistent file on $MOUNTPOINT:"
msgstr "Choisissez la taille en mégaoctets (maximum $MAXSIZE) pour le fichier persistent sur $MOUNTPOINT :"

#: create-slxsave-xfs:79 create-slxsave-xfs:100
#, sh-format
msgid "Error"
msgstr "Erreur"

#: create-slxsave-xfs:82
#, sh-format
msgid "The chosen size is too big ($SIZE > $MAXSIZE)"
msgstr "La taille choisie est trop grande ($SIZE > $MAXSIZE)"

#: create-slxsave-xfs:95
#, sh-format
msgid "slxsave.xfs ($SIZE MB) correctly created in $MOUNTPOINT"
msgstr "Le fichier slxsave.xfs ($SIZE Mo) a correctement été créé dans $MOUNTPOINT"

#: create-slxsave-xfs:103
#, sh-format
msgid "slxsave.xfs ($SIZE MB) cannot be created in $MOUNTPOINT"
msgstr "Le fichier slxsave.xfs ($SIZE Mo) n'a pas pu être créé dans $MOUNTPOINT"
