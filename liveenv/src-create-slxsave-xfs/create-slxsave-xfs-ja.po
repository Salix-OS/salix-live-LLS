#: ../../libexec/create-slxsave-xfs:30
#, sh-format
msgid "ERROR: You need to be root to run this"
msgstr "エラー：root·ログインしてから作動してください。"

#: ../../libexec/create-slxsave-xfs:35
#, sh-format
msgid "ERROR: zenity must be installed and in your PATH to run this"
msgstr "エラー：zenityをインストールし、正確にPATH設定をしてください。"

#: ../../libexec/create-slxsave-xfs:40
#, sh-format
msgid "ERROR: xfsprogs must be installed to run this"
msgstr "エラー：xfsprogsインストールしてから作動してください。"

#: ../../libexec/create-slxsave-xfs:49
#, sh-format
msgid "Mount point selection"
msgstr "マウント·ポイントの設定"

#: ../../libexec/create-slxsave-xfs:52
#, sh-format
msgid "Choose the mountpoint where to save the persistent file:"
msgstr "パーシスタント·ファイルをマウントをするポイントを選んでください。:"

#: ../../libexec/create-slxsave-xfs:53
#, sh-format
msgid "Mount point"
msgstr "マウント·ポイント"

#: ../../libexec/create-slxsave-xfs:54
#, sh-format
msgid "Size (in MB)"
msgstr "サイズ　(MB)"

#: ../../libexec/create-slxsave-xfs:69
#, sh-format
msgid "Size selection"
msgstr "サイズ設定"

#: ../../libexec/create-slxsave-xfs:72
#, sh-format
msgid ""
"Choose the size in megabytes (maximum $MAXSIZE) for persistent file on "
"$MOUNTPOINT:"
msgstr ""
"$MOUNTPOINTにマウントされるパーシスタント·ファイルのサイズ選んでください"
"(最高 $MAXSIZE):"

#: ../../libexec/create-slxsave-xfs:81 ../../libexec/create-slxsave-xfs:102
#, sh-format
msgid "Error"
msgstr "エラー"

#: ../../libexec/create-slxsave-xfs:84
#, sh-format
msgid "The chosen size is too big ($SIZE > $MAXSIZE)"
msgstr "選んだサイズが、大きすぎます ($SIZE > $MAXSIZE)。"

#: ../../libexec/create-slxsave-xfs:97
#, sh-format
msgid "slxsave.xfs ($SIZE MB) correctly created in $MOUNTPOINT"
msgstr "slxsave.xfs ($SIZE MB)を$MOUNTPOINTに作りました。"

#: ../../libexec/create-slxsave-xfs:105
#, sh-format
msgid "slxsave.xfs ($SIZE MB) cannot be created in $MOUNTPOINT"
msgstr "slxsave.xfs ($SIZE MB)を$MOUNTPOINTに作ることに失敗しました。"
