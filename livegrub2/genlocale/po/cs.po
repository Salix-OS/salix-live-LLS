#: genlocale:52 genlocale:63 genlocale:73
msgid "Start Salix Live"
msgstr "Zacnete s Salix Live"

#: genlocale:53 genlocale:64 genlocale:74
msgid "Memory Test"
msgstr "Paměťový test"

#: genlocale:54 genlocale:65 genlocale:75
msgid "Change language"
msgstr "Změnit jazyk"

#: genlocale:55 genlocale:66 genlocale:76
msgid "Change keyboard layout"
msgstr "Změnit rozložení klávesnice"

#: genlocale:56 genlocale:67 genlocale:77
msgid "Change boot options"
msgstr "Změnit nastavení bootování"

#: genlocale:57 genlocale:68 genlocale:78
msgid "Boot to runlevel '3' (text mode)"
msgstr "Bootovat do režimu 3 (textový mód)"

#: genlocale:58 genlocale:69 genlocale:79
msgid "Boot to runlevel '4' (graphical login) (default)"
msgstr "Bootovat do režimu 4 (grafické přihlášení) (základní)"

#: genlocale:59 genlocale:70 genlocale:80
msgid "Enable 'Copy everything to RAM'"
msgstr "Zapnout 'kopírovat vše do RAM'"

#: genlocale:60 genlocale:71 genlocale:81
msgid "Disable 'Copy everything to RAM' (default)"
msgstr "Vypnout 'kopírovat vše do RAM' (základní)"

#: genlocale:61 genlocale:72 genlocale:82
msgid "Enable numlock on boot"
msgstr "Zapnout numerickou klávesnici při bootování"

#: genlocale:62 genlocale:73 genlocale:83
msgid "Disable numlock on boot (default)"
msgstr "Vypnout numerickou klávesnici při bootování (základní)"

#: genlocale:63 genlocale:74 genlocale:84
msgid "Enable synaptics fdi (sometimes unreliable)"
msgstr "Zapnout synaptics fdi (občas nespolehlivé)"

#: genlocale:64 genlocale:75 genlocale:85
msgid "Disable synaptics fdi (reliable) (default)"
msgstr "Vypnout synaptics fdi (spolehlivé) (základní)"

#: genlocale:65 genlocale:76 genlocale:86
msgid "Return to boot menu"
msgstr "Návrat do boot menu"
